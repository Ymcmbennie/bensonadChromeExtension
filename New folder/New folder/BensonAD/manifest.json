{
    "manifest_version": 2,
    "name": "BensonAD Launcher",
    "description": "Quick launch to all my media",
    "version": "1.0.1",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}